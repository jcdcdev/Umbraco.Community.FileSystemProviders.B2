{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Storage": {
      "B2": {
        "Media": {
          "BucketName": "media",
          "ServiceUrl": "http://localhost:4566",
          "UseAccelerateEndpoint": false,
          "Credentials": {
            "ApplicationKey": "test",
            "KeyId": "test"
          }
        }
      }
    },
    "CMS": {
      "Global": {
        "SanitizeTinyMce": true,
        "Id": "e89eafa7-ace3-4b28-82fa-c12f96a3f473"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}