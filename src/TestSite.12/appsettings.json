{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Storage": {
      "B2": {
        "Media": {
          "BucketName": "media",
          "ServiceUrl": "http://localhost:4566",
          "UseAccelerateEndpoint": false,
          "Credentials": {
            "ApplicationKey": "test",
            "KeyId": "test"
          }
        }
      }
    },
    "CMS": {
      "Global": {
        "SanitizeTinyMce": true,
        "Id": "754dbe4e-9e15-4eca-9db5-45f8b32b893a"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  }
}